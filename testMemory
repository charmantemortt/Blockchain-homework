pragma solidity ^0.8.0;  

contract Memory{
    uint256 public numberPlus = 12;

    function testStorage(uint256 _number) public view returns(uint256){
        return _number + numberPlus;
    }
    

    function testMemory(uint256 _number) public pure returns(uint256){
        return _number * 2;
    }
}
